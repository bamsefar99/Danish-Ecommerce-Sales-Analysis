   "source": [
    "\n",
    "Calendar =\n",
    "ADDCOLUMNS (\n",
    "    CALENDARAUTO(),\n",
    "    \"Year\", YEAR([Date]),\n",
    "    \"Quarter\", \"Q\" & FORMAT([Date], \"Q\"),\n",
    "    \"Month\", FORMAT([Date], \"MMMM\"),\n",
    "    \"MonthNumber\", MONTH([Date]),\n",
    "    \"WeekNumber\", WEEKNUM([Date], 2)  // 2 = uge starter mandag\n",
    ")\n"
   ]
